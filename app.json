{
  "expo": {
    "name": "WhatsApp",
    "slug": "chatgeo",
    "version": "1.1.5",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "icon": "./assets/whatsappicon.jpg",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#008069"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.minichat.app2025",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.minichat.app2025",
      "adaptiveIcon": {
        "foregroundImage": "./assets/whatsappicon.jpg",
        "backgroundColor": "#008069"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/whatsappicon.jpg"
    },
    "scheme": "chatgeo",
    "plugins": [
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them.",
          "cameraPermission": "The app accesses your camera to let you take photos."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ee0e3120-19d9-4a8e-b54a-de4bdb7b3b85"
      }
    },
    "owner": "popscorn"
  }
}
