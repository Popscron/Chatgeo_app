{
  "expo": {
    "name": "Minichat",
    "slug": "minichat",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#008069"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.whatsappclone.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#008069"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "minichat",
    "plugins": [
      "expo-font",
      "expo-router"
    ]
  }
}